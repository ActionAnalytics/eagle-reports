[
{"$match": {"_schemaName": {"$eq": "Document"}}}
,{"$project": {
    "isTagged":{ "$cond": { "if": { "$and": 
        [
            { "$ne": [ "$type", "" ] }
            ,{ "$ne": [ "$type", null ] }
            ,{ "$eq": [ {"$type": "$type"}, "objectId" ] }
            ,{ "$ne": [ "$milestone", "" ] }
            ,{ "$ne": [ "$milestone", null ] }
            ,{ "$eq": [ {"$type": "$milestone"}, "objectId" ] }
            ,{ "$ne": [ "$documentAuthor", "" ] }
            ,{ "$ne": [ "$documentAuthor", null ] }
            ,{ "$eq": [ {"$type": "$documentAuthor"}, "objectId" ] }
        ]},
        "then": "Tagged",
        "else": "Untagged"
        }}
    }}
,{"$group":{"_id":{"projectName":"$projectName","isTagged":"$isTagged"},"count":{"$sum":1}}}
,{ "$project": {
    "_id":0
    ,"status": "$_id.isTagged"
    ,"count":"$count"
    }}
]